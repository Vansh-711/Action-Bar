{
  "name": "open_app_and_message_contact",
  "description": "Opens a specified application, searches for a contact, and sends a message to that contact.",
  "parameters": [
    "app",
    "contact",
    "message"
  ],
  "body": [
    {
      "action": "press_key",
      "key": "command+space"
    },
    {
      "action": "wait",
      "seconds": 1
    },
    {
      "action": "type_text",
      "text": "{app}"
    },
    {
      "action": "wait",
      "seconds": 1
    },
    {
      "action": "press_key",
      "key": "enter"
    },
    {
      "action": "wait",
      "seconds": 5
    },
    {
      "action": "press_key",
      "key": "command+f"
    },
    {
      "action": "wait",
      "seconds": 1
    },
    {
      "action": "type_text",
      "text": "{contact}"
    },
    {
      "action": "wait",
      "seconds": 1
    },
    {
      "action": "click_text",
      "text": "{contact}"
    },
    {
      "action": "wait",
      "seconds": 2
    },
    {
      "action": "type_text",
      "text": "{message}"
    },
    {
      "action": "press_key",
      "key": "enter"
    },
    {
      "action": "wait",
      "seconds": 1
    }
  ]
}